
@model THEcapstone.Models.FindServicesViewModel


@if (Model.Sitter.Count != 0)
{
    <table class="table">
        @foreach (var sitter in Model.Sitter)
        {

            <tr>
                <td>
                    @sitter.SitterFirstName
                </td>
                <td>
                    
                </td>
                <td>
                    @Html.ActionLink("View Profile", "ViewSitterProfile", new { id = sitter.SitterId })
                </td>
            </tr>
        }
    </table>

}
else
{

    <h1> No results </h1>



}