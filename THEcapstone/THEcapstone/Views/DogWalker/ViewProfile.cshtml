@model THEcapstone.Models.DWViewModel
    @{
        ViewBag.Title = "Profile";
        Layout = "~/Views/Shared/_WalkerLayout.cshtml";
    }

    <h2>Profile</h2>
    <hr />
    <br />
    <h4>Name: @Model.WalkerProf.WalkerFirstName  @Model.WalkerProf.WalkerLastName</h4>
    <br />
    <div class="row">
        <div class="col-md-12">
            <div class="panel panel-default">
                <div class="panel-heading">
                    <h3 class="panel-title">About Me</h3>
                </div>
                <div class="panel-body">
                    
                    <h4>@Model.WalkerProf.UserDiscription</h4>

                </div>
            </div>
        </div>       
    </div>
    <div class="row">
        <div class="col-lg-6">
            <div class="panel panel-default">
                <div class="panel-heading">
                    <h3 class="panel-title">Dog Preferences</h3>
                </div>
                <div class="panel-body">

                    <h4>@Model.WalkerProf.DogTypePreference</h4>

                </div>
            </div>        
        </div>
        
    </div>
    
        
   

